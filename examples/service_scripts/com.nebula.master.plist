<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.nebula.master</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/opt/nebula/bin/nebula</string>
		<string>-config</string>
		<string>/usr/local/etc/nebula/config.yml</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/nebula.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/nebula.log</string>
</dict>
</plist>
